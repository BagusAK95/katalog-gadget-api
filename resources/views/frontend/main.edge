<script>
    home()
    function home()
    {
        $.ajax({ 
            type: 'GET', 
            url: '/admin/home', 
            success: function (data) { 
               $('#content').html(data).show();
            }
        });
    }
    function brand()
    {
        $.ajax({ 
            type: 'GET', 
            url: '/admin/brand', 
            success: function (data) { 
               $('#content').html(data).show();
            }
        });
    }
    cek_token()
    function cek_token()
    {
         $.ajax({ 
            type: 'GET', 
            url: '/admin/gettoken', 
            success: function (data) { 
               $.ajax({ 
                type: 'GET', 
                url: 'http://127.0.0.1:3333/api/v1/user-detail', 
                headers: {
					'Authorization': 'Bearer ' + data.token
				},
                success: function (data) { 
                   $(".nama_user").html(data.name).show();
                }
                });
            },
            error: function ()
            {
                return false;
            }
            });
    }
    
    function logout()
        {
          window.location.href='/';
        }

</script>
